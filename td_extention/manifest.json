{
  "name": "Torrentday Search",
  "version": "1.0",
  "description": "Search torrents",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Torrentday Search",
      "default_icon": "zfb.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
